syntax on
filetype on
filetype plugin indent on
set listchars=tab:..
set list
set ww=[,],<,>,h,l,b
colorscheme solarized
set background=dark
set expandtab
set tabstop=2
set shiftwidth=2
set autoindent
set number

autocmd FileType python set omnifunc=pythoncomplete#Complete
set foldmethod=indent
set foldlevel=99

